#include <bits/stdc++.h>
#define ll long long

using namespace std;
void solution(){
    int n,m;
    cin>>n>>m;
    if((m-n)>=0)cout<<m-n<<endl;
    else cout<<0<<endl;
    
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    int t;
    cin>>t;
    while(t--){
        solution();
    }

    return 0;
}
