#include <bits/stdc++.h>
#define ll long long

using namespace std;


int main()
{
    int t;
    cin>>t;
    while(t--){
    int n,m;
    cin>>n>>m;
    if(m>n){
        for(int i=0;i<m;i++){
            cout<<i+1<<" ";
        }
        cout<<endl;
        int a=n+2;
        for(int i=1;i<n;i++){
            int b=a;
            cout<<b<<" ";
            for(int j=1;j<m;j++){
                cout<<b+i+1<<" ";
                b+=i+1;
            }
            a+=n+1;
            cout<<endl;
        }
        
        
        
    }
    else{
        int c;
        int d=m+1;
        for(int i=0;i<n;i++){
            c=i+1;
            cout<<c<<" ";
            for(int j=1;j<m;j++){
                cout<<c+d<<" ";
                c+=d;
            }
            cout<<endl;
            d++;
        }
        
        
    }
        
        
    
    }

    return 0;
}
