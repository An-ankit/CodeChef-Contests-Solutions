#include <bits/stdc++.h>
#define ll long long

using namespace std;
void solution(){
    int x;
    cin>>x;
    string s;
    cin>>s;
    for(int i=0;i<x;i++){
        if(s[i]=='A')s[i]='T';
        else if(s[i]=='T')s[i]='A';
        else if(s[i]=='G')s[i]='C';
        else s[i]='G';
    }
    cout<<s<<endl;
    
}

int main()
{
    int t;
    cin>>t;
    while(t--){
        solution();
    }

    return 0;
}
