#include <bits/stdc++.h>
#define ll long long

using namespace std;
void solution(){
    int s,x,y,z;
    cin>>s>>x>>y>>z;
    if((s-z)>=(x+y))cout<<0<<endl;
    else{
        if((s-z)>=x)cout<<1<<endl;
        else cout<<2<<endl;
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    int t;
    cin>>t;
    while(t--){
        solution();
    }

    return 0;
}
