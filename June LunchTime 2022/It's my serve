#include <bits/stdc++.h>
#define ll long long

using namespace std;
void solution(){
    int p,q;
    cin>>p>>q;
    int k=p+q;
    if(k%4==0 || k%4==1)cout<<"Alice"<<endl;
    else cout<<"Bob"<<endl;
    
}

int main()
{
    int t;
    cin>>t;
    while(t--){
        solution();
    }

    return 0;
}
